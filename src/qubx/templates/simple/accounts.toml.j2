# Account Configuration for {{ strategy_name }}
# Add your API credentials here before running live trading

# Example configuration for {{ exchange }}
[[accounts]]
name = "{{ strategy_name }}-live"
exchange = "{{ exchange }}"
api_key = "your_api_key_here"
secret = "your_secret_here"
testnet = false
base_currency = "USDT"
initial_capital = 100000.0

